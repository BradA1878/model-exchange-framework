# Optional: Workflow Configuration
WORKFLOW_SYSTEM_ENABLED=true
WORKFLOW_DEFAULT_TIMEOUT=30000
WORKFLOW_MAX_RETRIES=3
WORKFLOW_RETRY_BACKOFF=2
