{
  "name": "model-exchange-framework",
  "version": "1.1.0",
  "description": "Model Exchange Framework (MXF): Multi-agent AI orchestration framework with advanced optimization",
  "private": true,
  "scripts": {
    "start": "node -r module-alias/register dist/server/index.js",
    "start:dev": "npm run build && ts-node-dev --respawn --watch 'src/**/*' src/server/index.ts",
    "build": "tsc --project tsconfig.json",
    "build:dashboard": "cd dashboard && npm run build",
    "build:watch": "tsc --project tsconfig.json --watch",
    "clean": "rm -rf dist && find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete && find src -name '*.d.ts' -type f -delete",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "npm run build:dashboard",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint .",
    "demo:first-contact": "NODE_ENV=test ts-node examples/first-contact-demo/first-contact-demo.ts",
    "demo:interview": "NODE_ENV=test ts-node examples/interview-scheduling-demo/interview-scheduling-demo.ts",
    "demo:external-mcp": "NODE_ENV=test ts-node examples/external-mcp-registration/sdk-external-mcp-server-registration.ts",
    "demo:channel-mcp": "NODE_ENV=test ts-node examples/channel-mcp-registration/channel-mcp-demo.ts",
    "demo:fog-of-war": "NODE_ENV=test ts-node --project tsconfig.json examples/fog-of-war-game/connect-agents.ts",
    "test:meilisearch": "NODE_ENV=test ts-node tests/meilisearch-integration.test.ts",
    "test:code-execution": "NODE_ENV=test npx tsx tests/code-execution-demo.ts",
    "sdk:cli": "ts-node src/sdk/cli/mxf-cli.ts",
    "server:cli": "ts-node src/server/cli/server-cli.ts",
    "mxp:generate-key": "ts-node src/server/cli/generate-mxp-key.ts",
    "dev": "npm run docker:infra:up && npm run start:dev",
    "prod": "npm run docker:infra:up && npm run start",
    "dev:all": "docker-compose up -d",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose up -d --build",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:health": "docker-compose ps",
    "docker:infra:up": "(docker ps -q -f name=mxf-mongodb -f name=mxf-meilisearch -f name=mxf-redis | grep -q . && echo 'Infrastructure containers already running') || docker-compose --project-name mxf up -d mongodb meilisearch redis",
    "docker:infra:down": "docker-compose stop mongodb meilisearch redis",
    "docker:meilisearch:stats": "curl -X GET http://localhost:7700/stats -H \"Authorization: Bearer ${MEILISEARCH_MASTER_KEY}\"",
    "cleanup:db": "npm run cleanup:db",
    "sync-public": "bash sync-to-public.sh"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "orchestration",
    "llm",
    "mcp",
    "typescript",
    "framework",
    "mxf",
    "mxp"
  ],
  "author": "Brad Anderson <BradA1878@pm.me>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BradA1878/model-exchange-framework"
  },
  "bugs": {
    "url": "https://github.com/BradA1878/model-exchange-framework/issues"
  },
  "homepage": "https://github.com/BradA1878/model-exchange-framework#readme",
