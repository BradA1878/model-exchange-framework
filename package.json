{
  "name": "model-exchange-framework",
  "version": "1.5.1",
  "description": "Model Exchange Framework (MXF): Multi-agent AI orchestration framework with advanced optimization",
  "private": true,
  "scripts": {
    "start": "bun run dist/server/index.js",
    "start:dev": "bun --watch src/server/index.ts",
    "build": "tsc --project tsconfig.json",
    "build:dashboard": "cd dashboard && npm run build",
    "build:watch": "tsc --project tsconfig.json --watch",
    "clean": "rm -rf dist && find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete && find src -name '*.d.ts' -type f -delete",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "bun run build:dashboard",
    "rebuild": "bun run clean && bun run build",
    "lint": "eslint .",
    "demo:first-contact": "NODE_ENV=test bun run examples/first-contact-demo/first-contact-demo.ts",
    "demo:interview": "NODE_ENV=test bun run examples/interview-scheduling-demo/interview-scheduling-demo.ts",
    "demo:external-mcp": "NODE_ENV=test bun run examples/external-mcp-registration/sdk-external-mcp-server-registration.ts",
    "demo:channel-mcp": "NODE_ENV=test bun run examples/channel-mcp-registration/channel-mcp-demo.ts",
    "demo:fog-of-war": "NODE_ENV=test bun run examples/fog-of-war-game/connect-agents.ts",
    "demo:code-execution": "NODE_ENV=test bun run examples/code-execution-demo/code-execution-demo.ts",
    "demo:toon-optimization": "NODE_ENV=test bun run examples/toon-optimization-demo/toon-optimization-demo.ts",
    "demo:prompt-compaction": "NODE_ENV=test bun run examples/prompt-compaction-demo/prompt-compaction-demo.ts",
    "demo:inference-params": "NODE_ENV=test bun run examples/inference-params-demo/inference-params-demo.ts",
    "demo:workflow-patterns": "NODE_ENV=test bun run examples/workflow-patterns-demo/workflow-patterns-demo.ts",
    "demo:memory-strata": "NODE_ENV=test bun run examples/memory-strata-demo/memory-strata-demo.ts",
    "demo:mcp-prompts": "NODE_ENV=test bun run examples/mcp-prompts-demo/mcp-prompts-demo.ts",
    "demo:lsp-code-intelligence": "NODE_ENV=test bun run examples/lsp-code-intelligence-demo/lsp-code-intelligence-demo.ts",
    "demo:p2p-task-negotiation": "NODE_ENV=test bun run examples/p2p-task-negotiation-demo/p2p-task-negotiation-demo.ts",
    "demo:nested-learning": "NODE_ENV=test bun run examples/nested-learning-demo/nested-learning-demo.ts",
    "demo:muls": "NODE_ENV=test bun run examples/muls-demo/muls-demo.ts",
    "demo:orpar-memory": "NODE_ENV=test bun run examples/orpar-memory-demo/orpar-memory-demo.ts",
    "demo:dag": "NODE_ENV=test bun run examples/dag-demo/dag-demo.ts",
    "demo:kg": "NODE_ENV=test bun run examples/kg-demo/kg-demo.ts",
    "demo:tensorflow": "NODE_ENV=test TENSORFLOW_ENABLED=true bun run examples/tensorflow-demo/tensorflow-demo.ts",
    "demo:twenty-questions": "NODE_ENV=test TENSORFLOW_ENABLED=true bun run examples/twenty-questions/connect-agents.ts & PID=$!; cd examples/twenty-questions/client && npx vite --port 3007; kill $PID 2>/dev/null; wait $PID 2>/dev/null; lsof -ti:3006 | xargs kill -9 2>/dev/null",
    "test": "jest --config tests/jest.config.ts tests/integration",
    "test:integration": "jest --config tests/jest.config.ts tests/integration",
    "test:integration:manual": "jest --config tests/jest.config.ts tests/integration",
    "test:watch": "jest --config tests/jest.config.ts --watch tests/integration",
    "test:coverage": "jest --config tests/jest.config.ts --coverage",
    "test:ci": "jest --config tests/jest.config.ts tests/integration",
    "test:meilisearch": "NODE_ENV=test bun run tests/meilisearch-integration.test.ts",
    "test:unit": "jest --config tests/jest.unit.config.ts",
    "test:unit:watch": "jest --config tests/jest.unit.config.ts --watch",
    "test:unit:coverage": "jest --config tests/jest.unit.config.ts --coverage",
    "test:property": "jest --config tests/jest.unit.config.ts tests/property",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "sdk:cli": "bun run src/sdk/cli/mxf-cli.ts",
    "server:cli": "bun run src/server/cli/server-cli.ts",
    "mxp:generate-key": "bun run src/server/cli/generate-mxp-key.ts",
    "dev": "bun run test:unit && bun run docker:infra:up && bun --watch src/server/index.ts",
    "prod": "bun run docker:infra:up && bun run dist/server/index.js",
    "dev:all": "docker-compose up -d",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose up -d --build",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:health": "docker-compose ps",
    "docker:infra:up": "(docker ps -q -f name=mxf-mongodb -f name=mxf-meilisearch -f name=mxf-redis | grep -q . && echo 'Infrastructure containers already running') || docker-compose --project-name mxf up -d mongodb meilisearch redis",
    "docker:infra:down": "docker-compose stop mongodb meilisearch redis",
    "docker:meilisearch:stats": "curl -X GET http://localhost:7700/stats -H \"Authorization: Bearer ${MEILISEARCH_MASTER_KEY}\"",
    "sync-public": "bash sync-to-public.sh"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "orchestration",
    "llm",
    "mcp",
    "typescript",
    "framework",
    "mxf",
    "mxp"
  ],
  "author": "Brad Anderson <BradA1878@pm.me>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BradA1878/model-exchange-framework"
  },
